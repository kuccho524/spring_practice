<?xml version="1.0" encoding="UTF-8" ?>
<web-app xmls="htpp://java.sun.com/xml/ns/javaee"
	xmls:xsi="http://www.w3.org/2001/XMLSchema-instace"
	xsi:schemaLocation="
	  http://java.sun.com/xml/ns/javaee
	  htpp://java.sun.com/xml/ns/javaee/web-app_3_0.xsd
	"
	
    version="3.0">

	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<page-encoding>UTF-8</page-encoding>
			<include-prelude>/WEB-INF/include.jsp</include-prelude>
		</jsp-property-group>
	</jsp-config>
	
</web-app>